<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:lineSpacingMultiplier">1.25</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="TextView.color_000000">
        <item name="android:textColorHint">@color/color_CDCCCC</item>
        <item name="android:textColor">@color/color_000000</item>
    </style>

    <style name="TextView.color_000000.font_10sp">
        <item name="android:textSize">@dimen/font_10sp</item>
    </style>

    <style name="TextView.color_000000.font_12sp">
        <item name="android:textSize">@dimen/font_12sp</item>
    </style>

    <style name="TextView.color_000000.font_14sp">
        <item name="android:textSize">@dimen/font_14sp</item>
    </style>

    <style name="TextView.color_000000.font_16sp">
        <item name="android:textSize">@dimen/font_16sp</item>
    </style>

    <style name="TextView.color_000000.Bold">
        <item name="android:textStyle">bold</item>
    </style>


</resources>